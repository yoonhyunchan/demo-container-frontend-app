{
    "family": "yoon-frontend-task",
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048",
    "executionRoleArn": "ecsTaskExecutionRole",
    "requiresCompatibilities": [
        "FARGATE"
    ],

    "containerDefinitions": [
        {
            "name": "web",
            "image": "PLACEHOLDER",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}